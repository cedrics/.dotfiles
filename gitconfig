[alias]
  st = status
  ci = commit
  co = checkout
  sb = show-branch
  di = diff
  aa = add --all
  serve = daemon --verbose --base-path=. --export-all
  cont = rebase --continue
  amend = commit --amend
  unstaged = diff
  staged = diff --cached
  both = diff HEAD
  oneline = log --pretty=oneline
  tree = log --pretty=format:'%h %s' --graph --all -10
  l="!source ~/.gitcommands && git_l"
